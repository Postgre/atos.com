<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>apps/hook</name>
	<vendor>atos.com</vendor>
	<caption>Activity Hook</caption>
	<component-type>
		<vendor>sap.com</vendor>
		<type>Java</type>
	</component-type>
	<properties>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/bi/bp/javaLib</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>saplib</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>apps/ejb</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>client</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/library</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/library</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>archives</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>me/legacy</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>me/papi</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>me/wpmf/libs</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
	</dependencies>
	<folders>
		<package-folder>src</package-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="false">
			<sc-ref>
				<name>MEEXT</name>
				<vendor>atos.com</vendor>
			</sc-ref>
		</grant>
	</access-control-list>
	<attributes>
		<attribute>
			<attribute-type>ACHAssignment</attribute-type>
			<attribute-vendor>sap.com</attribute-vendor>
			<attribute-value>ATOS-EXT</attribute-value>
		</attribute>
	</attributes>
</development-component>
