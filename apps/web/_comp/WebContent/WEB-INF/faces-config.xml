<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    <managed-bean>
		<managed-bean-name>deptcodeTableBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>deptcodeTableBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customFieldsDataBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>customFieldsDataBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	
    <managed-bean>
		<managed-bean-name>materialCustomDataBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>materialCustomDataBeanConfigurator</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reasoncodeTableBeanConfigurator</managed-bean-name>
		<managed-bean-class>com.sap.me.wpmf.TableConfigurator</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>beanName</property-name>
			<value>reasoncodeTableBeanConfigurator</value>
		</managed-property>
	</managed-bean>
    <managed-bean>
		<managed-bean-name>orderSequenceDataBean</managed-bean-name>
		<managed-bean-class>com.atos.ordersequence.OrderSequenceDataController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{materialCustomDataBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderReleaseDataBeanVolvo</managed-bean-name>
		<managed-bean-class>com.atos.orderboardvolvo.OrderReleaseDataController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{materialCustomDataBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderSequencingDataBean</managed-bean-name>
		<managed-bean-class>com.atos.orderboardvolvo.sequncing.OrderReleaseDataController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{materialCustomDataBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderCreationDataBeanVolvo</managed-bean-name>
		<managed-bean-class>com.atos.orderboardvolvo.OrderCreationDataController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>		
	</managed-bean>
	<managed-bean>
		<managed-bean-name>orderReleaseBeanVolvo</managed-bean-name>
		<managed-bean-class>com.atos.orderboardvolvo.OrderReleaseController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>subAsseblyOrder</property-name>
			<value>#{subAssemblyOrderBeanVolvo}</value>
		</managed-property>		
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{materialCustomDataBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>subAssemblyOrderBeanVolvo</managed-bean-name>
		<managed-bean-class>com.atos.orderboardvolvo.SubAssemblyOrderController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>	
	</managed-bean>
	<managed-bean>
		<managed-bean-name>genericPatternCustomDataBean</managed-bean-name>
	<managed-bean-class>com.atos.genericpattern.GenericPatternDataController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>tableConfigBean</property-name>
			<value>#{materialCustomDataBeanConfigurator}</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
        <managed-bean-name>podConfiguratorFilterBean</managed-bean-name>
        <managed-bean-class>com.atos.refreshpod.TestConfiguratorFilter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>refreshPlugin</managed-bean-name>
        <managed-bean-class>com.atos.refreshpod.RefreshPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
	<managed-bean>
		<managed-bean-name>laboronPlugin</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.LaborOnOff</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>workinstrnPlugin</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.intelligentpod.LaborOnOff</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>assemblyPlugin</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.postscanactivities.AssemblyStatus</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>intelligentPodAssemblyPlugin</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.intelligentpod.postscanactivities.AssemblyStatus</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>podHeaderExtension</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.PodHeaderExtension</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>podHeaderExtensionForIntelligentPod</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.intelligentpod.PodHeaderExtension</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>miiTrasactionPlugin</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.miitrx.MIITrasactionPlugin</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
        <managed-bean-name>notificationMessageTestPlugin</managed-bean-name>
        <managed-bean-class>com.atos.sse.NotificationMessageTestPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>notificationMessageTestPlugin</value>
        </managed-property>
        <managed-property>
           <property-name>backingBeanName</property-name>
           <value>notificationMessageTestBackingBean</value>
        </managed-property>   
       </managed-bean>
    
    <managed-bean>
        <managed-bean-name>notificationMessageTestBackingBean</managed-bean-name>
        <managed-bean-class>com.sap.me.wpmf.PluginBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>notificationMessageTestBackingBean</value>
        </managed-property>
        <managed-property>
           <property-name>assignedBeanName</property-name>
           <value>notificationMessageTestPlugin</value>
        </managed-property> 
    </managed-bean> 
    <managed-bean>
        <managed-bean-name>nmPublisherPlugin</managed-bean-name>
        <managed-bean-class>com.atos.sse.NotificationMessagePublisherPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>nmPublisherPlugin</value>
        </managed-property>
        <managed-property>
           <property-name>backingBeanName</property-name>
           <value>nmPublisherBackingBean</value>
        </managed-property>
        <managed-property>
           <property-name>defaultUrl</property-name>
           <value>/com/atos/sse/nmPublisherPlugin.jsp</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>nmPublisherBackingBean</managed-bean-name>
        <managed-bean-class> com.sap.me.wpmf.PluginBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>nmPublisherBackingBean</value>
        </managed-property>
        <managed-property>
           <property-name>assignedBeanName</property-name>
           <value>nmPublisherPlugin</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>nmSubscriberPlugin</managed-bean-name>
        <managed-bean-class>com.atos.sse.NotificationMessageSubscriberPlugin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>nmSubscriberPlugin</value>
        </managed-property>
        <managed-property>
           <property-name>backingBeanName</property-name>
           <value>nmSubscriberBackingBean</value>
        </managed-property>
        <managed-property>
           <property-name>defaultUrl</property-name>
           <value>/com/atos/sse/nmSubscriberPlugin.jsp</value>
        </managed-property>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>nmSubscriberBackingBean</managed-bean-name>
        <managed-bean-class> com.sap.me.wpmf.PluginBackingBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
           <property-name>beanName</property-name>
           <value>nmSubscriberBackingBean</value>
        </managed-property>
        <managed-property>
           <property-name>assignedBeanName</property-name>
           <value>nmSubscriberPlugin</value>
        </managed-property>
    </managed-bean> 
    <managed-bean>
		<managed-bean-name>chassisassignbean</managed-bean-name>
		<managed-bean-class>com.atos.wpmf.web.podplugin.aggregate.ChassisSFCController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
	<managed-bean>
		<managed-bean-name>serialisation</managed-bean-name>
		<managed-bean-class>com.atos.serialization.SFCSerialization</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>kitDefinitionScreenControllerBean</managed-bean-name>
		<managed-bean-class>com.atos.kitting.KitDefinitionScreenConttroller</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
	<managed-bean>
		<managed-bean-name>kitAssignmentScreenControllerBean</managed-bean-name>
		<managed-bean-class>com.atos.kitting.KittingAssignmentScreenController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>assemblyDataScreenController</managed-bean-name>
		<managed-bean-class>com.atos.assembly_screen.AssemblyDataScreenController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
		<managed-bean>
		<managed-bean-name>serialisationNoLimit</managed-bean-name>
		<managed-bean-class>com.atos.serialization.SFCSerializationNoLimit</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

	</managed-bean>
       
</faces-config>
