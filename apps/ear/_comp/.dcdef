<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>apps/ear</name>
	<vendor>atos.com</vendor>
	<caption>EAR</caption>
	<component-type>
		<vendor>sap.com</vendor>
		<type>J2EE</type>
		<sub-type>Enterprise Application</sub-type>
	</component-type>
	<properties>
		<folder-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<folder-attribute attribute="meta-inf" path="META-INF"/>
		</folder-attributes>
		<j2ee-attributes xmlns="http://xml.sap.com/2002/11/DevelopmentComponent">
			<jee-version value="5.0"/>
		</j2ee-attributes>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/bi/bp/enterpriseApplication</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>enterpriseApplication</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>apps/ejb</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>ejbjar</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/hook</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>hook-assembly</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/idat</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>idat-assembly</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/library</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>api</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/library</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>archives</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/resources</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>resources-assembly</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/web</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>war</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>apps/webservice</name>
				<vendor>atos.com</vendor>
			</dc-ref>
			<pp-ref>war</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>me/ear</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<at-deploy-time/>
			<at-runtime/>
		</dependency>
	</dependencies>
	<folders>
		<source-folder>META-INF</source-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="false">
			<sc-ref>
				<name>MEEXT</name>
				<vendor>atos.com</vendor>
			</sc-ref>
		</grant>
	</access-control-list>
	<attributes>
		<attribute>
			<attribute-type>ACHAssignment</attribute-type>
			<attribute-vendor>sap.com</attribute-vendor>
			<attribute-value>ATOS-EXT</attribute-value>
		</attribute>
	</attributes>
</development-component>
